{
  "name": "tea_be",
  "version": "1.0.0",
  "description": "Tea Ordering Backend API",
  "main": "dist/server.js",
  "_moduleAliases": {
    "@config": "dist/config",
    "@features": "dist/features",
    "@shared": "dist/shared"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "start": "node -r module-alias/register dist/server.js",
    "seed": "ts-node -r tsconfig-paths/register src/scripts/seed.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "doppler:dev": "doppler secrets --config dev && doppler secrets download --no-file --format env --config dev > .env",
    "doppler:stg": "doppler secrets --config stg && doppler secrets download --no-file --format env --config stg > .env"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongodb": "^6.21.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "rate-limit-redis": "^4.3.1",
    "redis": "^5.11.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@types/redis": "^4.0.10",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
